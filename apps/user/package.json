{
  "name": "o2o-user",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:local": "dotenv -e .env.local -- next dev",
    "dev:dev": "mv .env.local .env.local.bak && mv .env.local .env.development.local.bak 2>/dev/null || true && dotenv -e .env.development -- next dev && mv .env.local.bak .env.local && mv .env.development.local.bak .env.local 2>/dev/null || true",
    "dev:prod": "dotenv -e .env.production --next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:check": "biome check src"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.74.7",
    "@vanilla-extract/css": "^1.17.1",
    "@vanilla-extract/recipes": "^0.5.5",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "ky": "^1.8.1",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-intersection-observer": "^9.16.0",
    "use-form-light": "^1.2.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webpack-env": "^1.18.8",
    "@vanilla-extract/css": "^1.17.1",
    "@vanilla-extract/next-plugin": "^2.4.10",
    "@vanilla-extract/vite-plugin": "^5.0.1",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.3",
    "dotenv-cli": "^8.0.0",
    "jsdom": "^26.1.0",
    "msw": "^2.7.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  },
  "msw": {
    "workerDirectory": ["public"]
  }
}
