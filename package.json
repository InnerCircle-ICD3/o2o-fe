{
  "name": "pnpm",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky",
    "postinstall": "chmod +x .husky/pre-commit",
    "dev:user": "pnpm -F o2o-user run dev",
    "dev:owner": "pnpm -F o2o-owner run dev",
    "dev": "pnpm -r dev",
    "build:user": "pnpm -F o2o-user build",
    "build:owner": "pnpm -F o2o-owner build",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:user": "pnpm -F o2o-user test",
    "test:owner": "pnpm -F o2o-owner test",
    "test:coverage:user": "pnpm -F o2o-user test:coverage",
    "test:coverage:owner": "pnpm -F o2o-owner test:coverage",
    "test:coverage": "pnpm -r test:coverage",
    "typecheck": "pnpm -r typecheck",
    "typecheck:user": "pnpm -F o2o-user typecheck",
    "typecheck:owner": "pnpm -F o2o-owner typecheck",
    "lint:check": "biome check apps/user/src apps/owner/src",
    "lint:fix": "biome check apps/user/src apps/owner/src --write"
  },
  "dependencies": {
    "@actions/github": "^6.0.0",
    "@slack/web-api": "^7.9.1",
    "@tanstack/react-query": "^5.74.7",
    "ky": "^1.8.1",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "use-form-light": "^1.2.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webpack-env": "^1.18.8",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.3",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "msw": "^2.7.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.10.0",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": ["biome check --write --no-errors-on-unmatched"]
  }
}
